{
	"name": "@digdir/illustration-lib",
	"version": "0.0.1",
	"description": "Framework-agnostic illustrations with light/dark mode support",
	"type": "module",
	"sideEffects": ["*.css"],
	"exports": {
		".": {
			"types": "./dist/react/index.d.ts",
			"import": "./dist/react/index.js"
		},
		"./react": {
			"types": "./dist/react/index.d.ts",
			"import": "./dist/react/index.js"
		},
		"./svg": {
			"types": "./dist/svg/index.d.ts",
			"import": "./dist/svg/index.js"
		},
		"./styles.css": "./index.css"
	},
	"files": ["dist", "index.css"],
	"publishConfig": {
		"access": "public"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/felleslosninger/tlp-svg-kit/tree/main/packages/illustration-lib"
	},
	"homepage": "https://felleslosninger.github.io/tlp-svg-kit/",
	"scripts": {
		"build": "npm run clean && tsc --project tsconfig.json && node tsc-build/index.js && tsc --project tsconfig.react.json && tsc --project tsconfig.svg.json",
		"clean": "rimraf dist && rimraf tsc-build"
	},
	"keywords": [
		"svg",
		"illustrations",
		"dark-mode",
		"light-mode",
		"react",
		"framework-agnostic"
	],
	"author": "digdir",
	"license": "ISC",
	"devDependencies": {
		"@svgr/core": "^8.1.0",
		"@svgr/plugin-jsx": "^8.1.0",
		"@svgr/plugin-prettier": "^8.1.0",
		"@svgr/plugin-svgo": "^8.1.0",
		"@types/node": "^22.19.9",
		"@types/react": "^19.2.13",
		"@types/react-dom": "^19.2.3",
		"rimraf": "^6.1.2",
		"svgo": "^4.0.0",
		"typescript": "^5.9.3"
	},
	"peerDependencies": {
		"react": ">=17.0.0"
	},
	"peerDependenciesMeta": {
		"react": {
			"optional": true
		}
	}
}
